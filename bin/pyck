#!/usr/bin/env python

import sys
import os


from pyck import pyck

def main():
   MyPyck = pyck.Pyck(sys.argv[1])
   Results = MyPyck.Run()
   print("\n".join(["%s \t: Installed" % line for line in Results[True]]))
   print("\n".join(["%s \t: Missing" % line for line in Results[False]]))



if __name__ == "__main__":
   main()
